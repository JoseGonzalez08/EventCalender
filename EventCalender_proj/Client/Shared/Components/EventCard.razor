@page "/calendar"
@using EventCalender_proj.Client.Shared.Models;
@using MudBlazor;
@inject IDialogService DialogService;
@using System.Collections.Generic;

<MudCard>
    <MudCardContent>
        <MudText>Name @model.Name</MudText>
        <MudText Typo="Typo.body2">Date @model.DateTime</MudText>
        <MudText Typo="Typo.body2">Description @model.Description</MudText>
    </MudCardContent>
    <MudCardActions>
    </MudCardActions>
</MudCard>
@code {
    [Parameter]
    public EventClass model { get; set; }


}
